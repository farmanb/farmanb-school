<?xml version="1.0" encoding="UTF-8" ?>
<coherence xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://xmlns.oracle.com/coherence/coherence-operational-config coherence-operational-config.xsd"
           xmlns="http://xmlns.oracle.com/coherence/coherence-operational-config">
   <cluster-config>
      <member-identity>
         <cluster-name system-property="tangosol.coherence.cluster">OrderCluster</cluster-name>
      </member-identity>
      <multicast-listener>
         <address>231.1.1.1</address>
         <port>7777</port>
         <time-to-live>0</time-to-live>
      </multicast-listener>
   </cluster-config>
   <configurable-cache-factory-config>
      <init-params>
         <init-param>
            <param-type>java.lang.String</param-type>
            <param-value system-property="tangosol.coherence.cacheconfig">coherence-cache-config.xml</param-value>
         </init-param>
      </init-params>
   </configurable-cache-factory-config>
</coherence>
