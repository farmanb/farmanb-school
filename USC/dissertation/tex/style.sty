%Commands
\newcommand{\op}[1]{\operatorname{#1}}

%Operators
\newcommand{\Ho}[1]{\op{Ho}\left(#1\right)}
\newcommand{\colim}{\op{colim}}
\newcommand{\coker}{\op{coker}}

%Exponents
\newcommand{\rqr}{{\op{rqr}}}
\newcommand{\opp}{{\op{op}}}

%Categories
\newcommand{\V}{\mathcal{V}}
\newcommand{\A}{\mathcal{A}}
\newcommand{\B}{\mathcal{B}}
\newcommand{\C}{\mathcal{C}}
\newcommand{\D}{\mathcal{D}}
\newcommand{\K}{\mathcal{K}}
\newcommand{\Serre}{\mathcal{S}}
\newcommand{\dgcat}[1]{\op{dgcat}_#1}
\newcommand{\DGCAT}[1]{\underline{\op{dgcat}}_{#1}}
\newcommand{\Ch}[1]{C\left(#1\right)}
\newcommand{\CH}[1]{\mathcal{C}\left(#1\right)}
\newcommand{\Mod}[1]{\op{Mod}#1}
\newcommand{\Ab}{\op{Ab}}
\newcommand{\Fun}{\op{Fun}}
\newcommand{\RHom}[1]{\mathbf{R}\underline{\op{Hom}}\left(#1\right)}
\newcommand{\RHomc}[1]{\mathbf{R}\underline{\op{Hom}}_c\left(#1\right)}
\newcommand{\Qcoh}[1]{\op{Qcoh}#1}

%Sets
\newcommand{\Z}{\mathbb{Z}}
\newcommand{\N}{\mathbb{N}}

%Graded Modules
\newcommand{\Gr}[1]{\op{Gr}#1 }
\newcommand{\GR}[1]{\underline{\op{Gr}}#1}
\newcommand{\QGr}[1]{\op{QGr}#1}
\newcommand{\QGR}[1]{\underline{\op{QGr}}#1}
\newcommand{\Tors}[1]{\op{Tors}#1}

%DG Modules
\newcommand{\dgMod}[1]{\op{dgMod}\left(#1\right)} %Left DG-Modules
\newcommand{\rdgMod}[1]{\ldgMod{#1^{\opp}}} %Right DG-modules; no need for ^\opp on the call.
\newcommand{\hproj}[1]{\op{h-proj}\left(#1\right)}
\newcommand{\hinj}[1]{\op{h-inj}\left(#1\right)}
\newcommand{\ldgGrMod}[1]{\mathcal{C}\left(\Gr{#1}\right)}
\newcommand{\rdgGrMod}[1]{\ldgGrMod{\left(#1^{\opp}\right)}}
\newcommand{\ldgQGrMod}[1]{\mathcal{C}\left(\QGr{#1}\right)}
\newcommand{\rdgQGrMod}[1]{\ldgQGrMod{#1^\opp}}
\newcommand{\dgBiGrMod}[1]{\mathcal{C}\left(#1\right)}

%Arrows
\def\ra{\rightarrow}

%Functors
\newcommand{\Ind}[1]{\op{Ind}_{#1}}
\newcommand{\Res}[1]{\op{Res}_{#1}}
\newcommand{\id}{\op{id}}
\newcommand{\Iso}[1]{\op{Iso}\left(#1\right)}
\newcommand{\Ext}{\operatorname{Ext}}
\newcommand{\EXT}{\operatorname{\underline{Ext}}}
